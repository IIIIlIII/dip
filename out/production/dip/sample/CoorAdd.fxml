<AnchorPane prefHeight="329.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ControllerAddCoor">
    <children>
        <TextField fx:id="AddCoorLong" layoutX="14.0" layoutY="151.0" prefHeight="26.0" prefWidth="460.0" promptText="Введите долготу" />
        <Button fx:id="OkayButton" layoutX="447.0" layoutY="289.0" mnemonicParsing="false" text="OK" />
